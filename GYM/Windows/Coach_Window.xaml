<Window x:Class="GYM.Windows.Coach_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GYM.Windows"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        Title="Coach_Window" Height="450" Width="800">
    <Grid>
        <DataGrid Name="DataGrid_Recording" HorizontalAlignment="Center" Height="231" Margin="0,26,0,0" VerticalAlignment="Top" Width="572" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <Button Content="Удалить" HorizontalAlignment="Left" Height="46" Margin="198,293,0,0" VerticalAlignment="Top" Width="112" Click="Delete_Click"/>
        <Button Content="Добавить" HorizontalAlignment="Left" Height="46" Margin="466,293,0,0" VerticalAlignment="Top" Width="112" Click="Add_Click"/>
        <Button Content="Назад" HorizontalAlignment="Left" Height="39" Margin="32,344,0,0" VerticalAlignment="Top" Width="99" Click="Back_Click"/>

    </Grid>
</Window>
